{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Nuages":
  {
    "Localization":
    {
      "DefaultCulture": "en-CA",
      "LangClaim": "lang",
      "MissingTranslationUrl" : null,
      "Cultures": ["fr-CA","en-CA"]
    }
  }
}
